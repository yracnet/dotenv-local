{
  "name": "dotenv-local",
  "version": "1.0.2",
  "type": "module",
  "description": "A utility library for loading local environment variables with prioritized file loading in Node.js and Vite projects. Extends dotenv functionality for better environment management in various modes like development, production, and testing.",
  "keywords": [
    "vite",
    "vite-plugin",
    "vite-plugin-loadenv",
    "dotenv",
    "loadenv",
    "loadenv-local",
    "environment-variables",
    "local-development",
    "vite-config",
    "config-management",
    "node",
    "env",
    "env-local",
    "load",
    "nodejs"
  ],
  "files": [
    "dist",
    "client.d.ts",
    "LICENSE"
  ],
  "license": "MIT",
  "author": "Willyams Yujra <yracnet@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/yracnet/dotenv-local"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "homepage": "https://github.com/yracnet/dotenv-local",
  "bugs": {
    "url": "https://github.com/yracnet/dotenv-local/issues"
  },
  "scripts": {
    "build": "tsup",
    "prepublish": "tsup",
    "test": "jest"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "tsup": "^5.0.0",
    "typescript": "^5.3.3",
    "jest": "^29.6.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/yracnet"
  }
}
